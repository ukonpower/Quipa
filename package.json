{
  "name": "quipa",
  "version": "1.0.0",
  "description": "シンプルなIPA配信CLIツール - ゼロコンフィグでOTA配信",
  "main": "dist/cli.js",
  "bin": {
    "quip": "./dist/cli.js"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:binary",
    "build:ts": "tsc",
    "build:binary": "mkdir -p bin/macos/apple-silicon bin/macos/intel && pkg . --targets node18-macos-arm64 --output bin/macos/apple-silicon/quipa && pkg . --targets node18-macos-x64 --output bin/macos/intel/quipa",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "prepare": "npm run build:ts"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": []
  },
  "keywords": [
    "ios",
    "ipa",
    "ota",
    "distribution",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bplist-parser": "^0.3.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "plist": "^3.1.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/plist": "^3.0.5",
    "@types/yauzl": "^2.10.3",
    "pkg": "^5.8.1",
    "typescript": "^5.3.3"
  }
}
