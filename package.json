{
  "name": "quipa",
  "version": "2.0.0",
  "description": "シンプルなIPA配信CLIツール - ゼロコンフィグでOTA配信",
  "main": "dist/cli.js",
  "bin": {
    "quip": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "prepare": "npm run build",
    "package": "npm run build && pkg . --out-path bin",
    "package:macos": "npm run build && pkg . --targets node18-macos-x64,node18-macos-arm64 --out-path bin"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [],
    "outputPath": "bin"
  },
  "keywords": [
    "ios",
    "ipa",
    "ota",
    "distribution",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "ngrok": "^5.0.0-beta.2",
    "plist": "^3.1.0",
    "qrcode-terminal": "^0.12.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/plist": "^3.0.5",
    "@types/yauzl": "^2.10.3",
    "pkg": "^5.8.1",
    "typescript": "^5.3.3"
  }
}
